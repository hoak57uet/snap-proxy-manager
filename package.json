{
  "name": "snap-proxy-manager",
  "private": true,
  "version": "1.0.0",
  "description": "Advanced IPv6 Proxy Manager with Rotation Support",
  "author": {
    "name": "Snap Proxy Team",
    "email": "support@snapproxy.com"
  },
  "homepage": "https://snapproxy.com",
  "repository": {
    "type": "git",
    "url": "https://github.com/your-org/snap-proxy-manager.git"
  },
  "license": "MIT",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "npm run clean && npm run download-binaries && npm run compile && npm run build:renderer && npm run build:electron",
    "build:dev": "npm run download-binaries && tsc && vite build && electron-builder --publish=never",
    "build:prod": "npm run clean && npm run download-binaries && npm run compile && npm run build:renderer && npm run build:electron --publish=always",
    "build:renderer": "vite build",
    "build:electron": "electron-builder",
    "compile": "tsc --noEmit",
    "clean": "rimraf dist dist-electron release",
    "download-binaries": "node scripts/download-3proxy.js",
    "postinstall": "npm run download-binaries",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "lint:fix": "eslint . --ext ts,tsx --fix",
    "preview": "vite preview",
    "test": "echo \"No tests specified\" && exit 0",
    "pack": "electron-builder --dir",
    "dist": "electron-builder --publish=never",
    "dist:mac": "electron-builder --mac --publish=never",
    "dist:win": "electron-builder --win --publish=never",
    "dist:linux": "electron-builder --linux --publish=never",
    "release": "node scripts/build-release.js",
    "version:patch": "node scripts/version-bump.js patch",
    "version:minor": "node scripts/version-bump.js minor",
    "version:major": "node scripts/version-bump.js major"
  },
  "dependencies": {
    "@ant-design/icons": "^6.1.0",
    "@vscode/sudo-prompt": "^9.3.1",
    "antd": "^5.27.4",
    "dayjs": "^1.11.18",
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  },
  "devDependencies": {
    "@types/react": "^18.2.64",
    "@types/react-dom": "^18.2.21",
    "@typescript-eslint/eslint-plugin": "^7.1.1",
    "@typescript-eslint/parser": "^7.1.1",
    "@vitejs/plugin-react": "^4.2.1",
    "electron": "^38.2.2",
    "electron-builder": "^24.13.3",
    "electron-updater": "^6.1.7",
    "eslint": "^8.57.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.5",
    "rimraf": "^5.0.5",
    "typescript": "^5.2.2",
    "vite": "^7.1.10",
    "vite-plugin-electron": "^0.28.6",
    "vite-plugin-electron-renderer": "^0.14.5"
  },
  "main": "dist-electron/main.js"
}
